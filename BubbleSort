public class BubbleSort 
{
	public static void main(String[] args) 
	{
		int temp[]={12,46,46,22,1,89}

		for(int i=0;i<temp.length-1;i++)
		{
			for(int j=0;j<temp.lenth-1-i;j++)
			{
				if(temp[j]>temp[j+1])
				{
					int num=temp[j];
					temp[j+1]=num;
					temp[j]=temp[j+1];
				}
			
			
			}

		
		System.out.println(temp);
		
		
		}
		
	}
}
